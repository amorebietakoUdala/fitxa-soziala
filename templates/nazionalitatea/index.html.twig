{% extends 'base.html.twig' %}

{% block title %}{{"Nazionalitatea aukerak"|trans }}{% endblock %}

{% block body %}
    <main role="main" class="container">
        
        <div class="card">
        
        <div class="card-body">
            <h5 class="card-title">"{{ "Nazionalitatea" |trans}}" {{ "aukerak" |trans}}</h5>
    
            
            
            {% for nazionalitatea in nazionalitateas %}
            <div class="row maila{{nazionalitatea.lvl}}">
                
                <div class="col">
                    <span class="label label-default">{{ nazionalitatea.mailaEu }}</span>
                </div>
                <div class="col">
                    <span class="label label-default">{{ nazionalitatea.mailaEs }}</span>
                </div>
                <div class="col-1">
                    <span class="label label-default">
                    <a href="{{ path('nazionalitatea_edit', {'id': nazionalitatea.id}) }}">{{ "Edit" |trans}}</a>
                    </span>
                </div>
            </div>
            {% else %}
                <span class="label label-default">
                    <td colspan="7">no records found</td>
                </span>
            {% endfor %}
            
            <div class="row">
                <div class="col">
                    <a href="{{ path('nazionalitatea_new') }}">{{ "berria" |trans}}</a>
                </div>
            </div>
        </div
    </main>

    
{% endblock %}
